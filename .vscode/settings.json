{
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "html",
      "vue"
    ],
    "eslint.enable": true,
    "eslint.alwaysShowStatus": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": true
    },
    "editor.detectIndentation": false,
    "editor.tabSize": 2,
    "css.validate": false,
    "eslint.codeAction.showDocumentation": {
      "enable": true
    },
    "editor.rulers": [
      120
    ],
    "editor.selectionHighlight": true,
  }
